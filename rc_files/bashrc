export PATH=\
/usr/local/bin:\
/usr/bin:\
/bin:\
/usr/local/go/bin:\
$HOME/.local/bin/scripts:\
/usr/local/games:\
/usr/games:\
$HOME/.cargo/bin
	
export OSH=$HOME/.oh-my-bash
export RUST_SRC_PATH="$(rustc --print sysroot)/lib/rustlib/src/rust/library"

OSH_THEME="garo"

completions=(
  git
  composer
  ssh
)

aliases=(
  general
)

plugins=(
  git
  bashmarks
)

source $OSH/oh-my-bash.sh

alias '?'=duck

source "$HOME/.cargo/env"
